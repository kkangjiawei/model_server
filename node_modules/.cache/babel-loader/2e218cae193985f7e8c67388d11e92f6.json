onents/Dropzone'\n\nexport default {\n  name: 'DropzoneDemo',\n  components: { Dropzone },\n  methods: {\n    dropzoneS(file) {\n      console.log(file)\n      this.$message({ message: 'Upload success', type: 'success' })\n    },\n    dropzoneR(file) {\n      console.log(file)\n      this.$message({ message: 'Delete success', type: 'success' })\n    }\n  }\n}\n</script>\n\n"],"mappings":";;;;;;;;;;;;;AAaA,OAAAA,QAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAF,QAAA,EAAAA;EAAA;EACAG,OAAA;IACAC,SAAA,WAAAA,UAAAC,IAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,IAAA;MACA,KAAAG,QAAA;QAAAC,OAAA;QAAAC,IAAA;MAAA;IACA;IACAC,SAAA,WAAAA,UAAAN,IAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,IAAA;MACA,KAAAG,QAAA;QAAAC,OAAA;QAAAC,IAAA;MAAA;IACA;EACA;AACA"}]}